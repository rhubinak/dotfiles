[calendars]
[[calendar]]
path = ~/.config/vdirsyncer/calendars/
color = dark green

[[birthdays]]
path = ~/.config/vdirsyncer/calendars_monica/
color = dark cyan

[locale]
local_timezone= Europe/Prague
default_timezone= Europe/Prague
timeformat= %H:%M
dateformat= %d.%m.
longdateformat= %d.%m.%Y
datetimeformat= %d.%m. %H:%M
longdatetimeformat= %d.%m.%Y %H:%M
weeknumbers=left
